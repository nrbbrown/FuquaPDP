<%@ggoals.each do |onegoal|%>
	<div class="effortRow effortRow3">
		<div class="goalRowBox">
			<div class="effortRowTitle"><%=onegoal.goal%></div>
			<div class="effortRowProgress"><%=@goalScore[onegoal.id]%></div>
		</div>
	</div>
	<%if @goalScore[onegoal.id] != 'N/A' %>
		<%onegoal.tasks.each do |onetask|%>
			<div class="effortRow effortRow4">
				<div class="taskRowBox">
					<div class="effortRowTitle"><%=onetask.task%></div>	
					<div class="effortRowProgress"><%=@tasksEffortScore[onetask.id]%></div>
				</div>
			</div>
		<%end%>
	<%end%>
<%end%>