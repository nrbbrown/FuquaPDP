<div id="outer-score-container">
  <%= render 'header' %>


	<div class="effortBox">
		<div class="effortScoreBox">
			<div class="titleBoxInner">
				Effort Score
			</div>
			<div class="effortInnerBox">
				<div class="effortRow overallRow">
					<div class="effortRowTitle">Overall</div>
					<div class="effortRowProgress"><%=@overallScore%>%</div>
				</div>

                <%@domainType = 1%>
				<div class="effortRow effortRow2">
					<div class="categoryRowBox" id="category_row_<%=@domainType%>" <% if @domainScore["academic"] != 'N/A' %> style="cursor:pointer;" onclick="openScoreCardDetails(<%=@domainType%>,3,0)"  <% else %> style="border-bottom: 2px solid #999999"  <% end %>>
                      <div class="openPlusBox"><% if @domainScore["academic"] != 'N/A' %><span id="plus_div_3_<%=@domainType%>_0" class="openPlusDiv">[+]</span><% end%></div>
                      <div class="effortRowTitle">Academic</div>
					  <div class="effortRowProgress"><%=@domainScore["academic"]%></div>
					</div>
 				</div>
				<%@ggoals = @academicGoals%>
				<%= render 'goaleffort' %>

                <%@domainType = 2%>
				<div class="effortRow effortRow2">
                  <div class="categoryRowBox" id="category_row_<%=@domainType%>" <% if @domainScore["career"] != 'N/A' %> style="cursor:pointer;" onclick="openScoreCardDetails(<%=@domainType%>,3,0)" <% else %> style="border-bottom: 2px solid #999999"  <% end %>>
                        <div class="openPlusBox"><% if @domainScore["career"] != 'N/A' %><span id="plus_div_3_<%=@domainType%>_0" class="openPlusDiv">[+]</span><% end%></div>
						<div class="effortRowTitle">Career</div>
						<div class="effortRowProgress"><%=@domainScore["career"]%></div>
					</div>
				</div>
				<%@ggoals = @careerGoals%>
				<%= render 'goaleffort' %>

                <%@domainType = 3%>
				<div class="effortRow effortRow2">
                  <div class="categoryRowBox" id="category_row_<%=@domainType%>" <% if @domainScore["social"] != 'N/A' %> style="cursor:pointer;" onclick="openScoreCardDetails(<%=@domainType%>,3,0)" <% else %> style="border-bottom: 2px solid #999999"  <% end %>>
                        <div class="openPlusBox"><% if @domainScore["social"] != 'N/A' %><span id="plus_div_3_<%=@domainType%>_0" class="openPlusDiv">[+]</span><% end%></div>
                        <div class="effortRowTitle">Social</div>
						<div class="effortRowProgress"><%=@domainScore["social"]%></div>
					</div>
				</div>
				<%@ggoals = @socialGoals%>
				<%= render 'goaleffort' %>

                <%@domainType = 4%>
                <div class="effortRow effortRow2">
                    <div class="categoryRowBox" id="category_row_<%=@domainType%>" <% if @domainScore["personal"] != 'N/A' %> style="cursor:pointer;" onclick="openScoreCardDetails(<%=@domainType%>,3,0)" <% else %> style="border-bottom: 2px solid #999999" <% end %>>
                        <div class="openPlusBox"><% if @domainScore["personal"] != 'N/A' %><span id="plus_div_3_<%=@domainType%>_0" class="openPlusDiv">[+]</span><% end%></div>
                        <div class="effortRowTitle">Personal</div>
						<div class="effortRowProgress"><%=@domainScore["personal"]%></div>
					</div>
				</div>
				<%@ggoals = @personalGoals%>
				<%= render 'goaleffort' %>

                <%@domainType = 5%>
                <div class="effortRow effortRow2">
                     <div class="categoryRowBox" id="category_row_<%=@domainType%>" <% if @domainScore["physical"] != 'N/A' %> style="cursor:pointer;" onclick="openScoreCardDetails(<%=@domainType%>,3,0)" <% else %> style="border-bottom: 2px solid #999999"  <% end %>>
                        <div class="openPlusBox"><% if @domainScore["physical"] != 'N/A' %><span id="plus_div_3_<%=@domainType%>_0" class="openPlusDiv">[+]</span><% end%></div>
                        <div class="effortRowTitle">Physical</div>
						<div class="effortRowProgress"><%=@domainScore["physical"]%></div>
					</div>
				</div>
				<%@ggoals = @physicalGoals%>
				<%= render 'goaleffort' %>
				<div class="effortRow overallRowBottom">
					
				</div>
			</div>
		</div>

	</div>
	

</div>